{
  "name": "sigmaitc.github.io",
  "version": "1.0.0",
  "description": "Main site for Sigma IT Consulting open source project",
  "main": "index.html",
  "browserslist": [
    "ie >= 11",
    "> 1%",
    "last 2 versions"
  ],
  "config": {
    "pre-git": {
      "commit-msg": "",
      "pre-commit": [],
      "pre-push": "npm test",
      "post-commit": "git status",
      "post-checkout": [],
      "post-merge": "yarn"
    }
  },
  "scripts": {
    "build": "webpack -p",
    "commit": "commit-wizard",
    "lint:css": "stylelint sass/**/*.scss",
    "lint:js": "eslint . --ext .js --ext .json --ignore-path .gitignore",
    "lint:yaml": "yamllint .travis.yml **/*.yml **/**/*.yml",
    "test": "run-s lint:js",
    "upgrade": "updtr --save-exact"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SigmaITM/sigmaitm.github.io.git"
  },
  "author": "Sigma IT Consulting",
  "contributors": [
    "Chris Sundberg <chris.sundberg@sigma.se>"
  ],
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/SigmaITM/sigmaitm.github.io/issues"
  },
  "homepage": "https://github.com/SigmaITM/sigmaitm.github.io#readme",
  "dependencies": {
    "jquery": "3.1.1",
    "materialize-css": "0.98.0"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-es2015": "6.22.0",
    "css-loader": "0.26.2",
    "cssnano": "3.10.0",
    "eslint": "3.16.1",
    "eslint-plugin-json": "1.2.0",
    "extract-text-webpack-plugin": "2.0.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "postcss": "5.2.15",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.3.2",
    "postcss-reporter": "3.0.0",
    "pre-git": "3.14.0",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.2",
    "stylelint": "7.9.0",
    "stylelint-scss": "1.4.3",
    "updtr": "0.2.3",
    "webpack": "2.2.1",
    "yaml-lint": "0.0.4"
  }
}
